<div class="container-fluid">
  <div class="row ps-0 pe-0 app-row">
    <div class="col-2 app-sidebar">
      <h5 class="heading-text">Liste de tous les appareils</h5>
      <div class="d-flex align-items-start justify-content-between ps-1 pe-1">
        <form id="form2">
          <div class="form-group mb-3 row">
            <div
              class="d-flex align-items-start justify-content-center align-self-start">
              <input
                type="text"
                class="form-control form-control-sm"
                id="recherche"
                name="code"
                aria-describedby="input1_help"
                placeholder="Recherche" />
            </div>
          </div>
        </form>
      </div>
      <button id="btn2" class="btn disabled" dmx-show="loadingTsp.value">
        <span class="spinner-border spinner-border-sm" role="status"></span
        >chargement...
      </button>

      <div class="d-flex flex-column" dmx-show="form2.recherche.value==null">
        <div class="table-responsive">
          <table
            class="table table-striped table-bordered table-hover table-sm">
            <thead>
              <tr>
                <th>Appareil</th>
                <th>Code tsp</th>
              </tr>
            </thead>
            <tbody
              is="dmx-repeat"
              dmx-generator="bs5table"
              dmx-bind:repeat="tspViewData.data"
              id="allTspBody"
              dmx-state="offset"
              dmx-sort="offset"
              dmx-order="offset"
              key="IdTsp">
              <tr
                dmx-on:click="selectedTspId.setValue(IdTsp);accessoiresData.load({id: IdTsp});suivibordereaudata.load({id: IdTsp});logAffectationTspData.load({id: IdTsp});showDetails.setValue(0);affectationByTsp.load({})"
                dmx-class:bg-info="affectation!==null"
                dmx-class:text-white="affectation!==null">
                <td dmx-text="Nom"></td>
                <td dmx-text="CodeTsp"></td>
              </tr>
            </tbody>
          </table>
        </div>
        <ul
          class="pagination pagination-sm"
          dmx-dataview="tspViewData"
          dmx-generator="bs5paging">
          <li
            class="page-item"
            dmx-class:disabled="tspViewData.page == 1"
            aria-label="First">
            <a
              href="javascript:void(0)"
              class="page-link"
              dmx-on:click="tspViewData.first()"
              ><span aria-hidden="true">&lsaquo;&lsaquo;</span></a
            >
          </li>
          <li
            class="page-item"
            dmx-class:disabled="tspViewData.page == 1"
            aria-label="Previous">
            <a
              href="javascript:void(0)"
              class="page-link"
              dmx-on:click="tspViewData.prev()"
              ><span aria-hidden="true">&lsaquo;</span></a
            >
          </li>
          <li
            class="page-item"
            dmx-class:active="title == tspViewData.page"
            dmx-class:disabled="!active"
            dmx-repeat="tspViewData.getDataViewPagination(2,1,'...')">
            <a
              href="javascript:void(0)"
              class="page-link"
              dmx-on:click="tspViewData.select(page)"
              >{{title)}}</a
            >
          </li>
          <li
            class="page-item"
            dmx-class:disabled="tspViewData.page == tspViewData.pages"
            aria-label="Next">
            <a
              href="javascript:void(0)"
              class="page-link"
              dmx-on:click="tspViewData.next()"
              ><span aria-hidden="true">&rsaquo;</span></a
            >
          </li>
          <li
            class="page-item"
            dmx-class:disabled="tspViewData.page == tspViewData.pages"
            aria-label="Last">
            <a
              href="javascript:void(0)"
              class="page-link"
              dmx-on:click="tspViewData.last()"
              ><span aria-hidden="true">&rsaquo;&rsaquo;</span></a
            >
          </li>
        </ul>
      </div>
      <div class="d-flex flex-column" dmx-show="form2.recherche.value!==null">
        <div class="table-responsive">
          <table class="table table-hover table-sm">
            <thead>
              <tr>
                <th>Appareil</th>
                <th>Identifiant</th>
              </tr>
            </thead>
            <tbody
              is="dmx-repeat"
              dmx-generator="bs5table"
              dmx-bind:repeat="searchData.data"
              id="tableRepeat1">
              <tr
                dmx-on:click="selectedTsp.setValue($value)"
                dmx-class:clicked="selectedTsp.value==$value">
                <td dmx-text="Nom"></td>
                <td dmx-text="IdTsp"></td>
              </tr>
            </tbody>
          </table>
        </div>
        <ul
          class="pagination pagination-sm flex-wrap"
          dmx-dataview="searchData"
          dmx-generator="bs5paging">
          <li
            class="page-item"
            dmx-class:disabled="searchData.page == 1"
