<div class="modal" id="addTspModal" is="dmx-bs5-modal" tabindex="-1">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-body">
        <form
          method="post"
          is="dmx-api-form"
          id="apiform1"
          action="http://localhost:5243/api/tsp"
          post-data="json"
          dmx-on:success="notifies1.success('Nouveau Tsp enregistré');scheduler1.start();addTspModal.toggle()"
          dmx-on:error="notifies1.danger(lastError.response+lastError.message);scheduler1.start();loading.setValue(false)"
          dmx-header:authorization="connectedUser.data.token">
          <div class="row justify-content-center">
            <div class="col-12">
              <h4 class="heading-text">Nouveau Tsp</h4>
            </div>
            <div class="col-10">
              <div class="d-flex flex-row justify-content-around">
                <div class="d-flex justify-content-between flex-column">
                  <div class="form-group mb-3">
                    <label for="modelName" class="form-label"
                      >Nom et Id du nouveau TSP</label
                    >
                    <input
                      id="IdTsp"
                      name="IdTsp"
                      class="form-control mb-1 form-control-plaintext"
                      dmx-bind:value="nomNouveauAppareil.data"
                      type="hidden" />
                    <input
                      id="Nom"
                      name="Nom"
                      class="form-control form-control-plaintext mb-0 form-control-sm"
                      dmx-bind:value="'Z'+nomNouveauAppareil.data"
                      readonly="true" />
                  </div>
                  <div class="form-group mb-3">
                    <label for="modelName" class="form-label"
                      >Le code TSP</label
                    >
                    <input
                      id="CodeTsp"
                      name="CodeTsp"
                      class="form-control form-control-plaintext form-control-sm"
                      dmx-bind:value="Nom.value"
                      readonly="true" />
                  </div>
                  <input
                    id="UserCrea"
                    name="UserCrea"
                    type="hidden"
                    class="form-control"
                    dmx-text="" />

                  <div class="form-group mb-3">
                    <label for="selectModel" class="form-label"
                      >Le model de TSP</label
                    >
                    <input
                      id="ModeleId"
                      name="ModeleId"
                      class="form-control"
                      type="hidden" />
                    <select
                      id="selectModel"
                      class="form-select form-control form-select-sm"
                      dmx-bind:options="listModelTsp.data"
                      optiontext="Nom"
                      optionvalue="IdModele"
                      dmx-on:changed="ModeleId.setValue(value)"
                      name="ModeleId">
                      <option selected="" value=""></option>
                    </select>
                  </div>
                </div>
                <div class="d-flex justify-content-between flex-column">
                  <div class="form-group mb-3">
                    <label for="modelName" class="form-label"
                      >Année de production</label
                    >
                    <input
                      id="AnneeProduction"
                      name="AnneeProduction"
                      class="form-control border form-control-plaintext mb-0 form-control-sm" />
                  </div>
                  <div class="form-group">
                    <label for="modelName" class="form-label"
                      >Date d'achat</label
                    >
                    <input
                      id="text1"
                      name="DateAchat"
                      class="form-control form-control-sm"
                      is="dmx-date-picker"
                      applylabel="Enregistrer"
                      cancellabel="Annuler"
                      format="DD-M-Y" />
                  </div>
                  <div class="form-group mb-3">
                    <label for="modelName" class="form-label"
                      >Numéro de serie</label
                    >
                    <input
                      id="NumSerie"
                      name="NumSerie"
                      class="form-control border form-control-plaintext mb-0 form-control-sm" />
                  </div>
                </div>
                <div class="d-flex flex-column justify-content-start">
                  <div class="form-group mb-3">
                    <label for="modelName" class="form-label"
                      >Description</label
                    >
                    <input
                      id="Description"
                      name="Description"
                      class="form-control border form-control-plaintext mb-0 form-control-sm" />
                  </div>
                </div>
              </div>
            </div>

            <div class="col-10">
              <div class="d-flex justify-content-around">
                <button
                  id="submit"
                  class="btn btn-primary btn-sm"
                  type="submit"
                  dmx-bind:disabled="!ModeleId.value||!SectEmp.value"
                  dmx-on:click="loading.setValue(true)">
                  Enregistrer
                </button>

                <button
                  id="cancel"
                  class="btn btn-warning btn-sm"
                  data-bs-dismiss="modal">
                  Annuler
                </button>
              </div>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
