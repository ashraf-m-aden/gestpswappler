<dmx-notifications id="notifies1"></dmx-notifications>
<div class="modal" id="affecterUserModal" is="dmx-bs5-modal" tabindex="-1">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title heading-text">
          Affecter un agent à l'appareil: {{ selectedTsp.value.Nom }}
        </h5>
      </div>
      <div class="modal-body">
        <form
          is="dmx-api-form"
          id="affectationForm"
          method="post"
          action="http://localhost:5243/api/affectationAm"
          dmx-header:authorization="connectedUser.data.token"
          dmx-on:success="notifies1.success('L\'appareil a été affecté avec succés');tsp.load({});selectedTsp.setValue(tsp.data.where(`IdTsp`, selectedTsp.value, '=='));affecterUserModal.toggle()"
          dmx-on:error="notifies1.danger(lastError.response)">
          <input
            id="IdTspAffect"
            name="IdTsp"
            type="hidden"
            class="form-control"
            dmx-bind:value="selectedTsp.value.IdTsp" />
          <input
            id="UserCreaAffect"
            name="UserCrea"
            type="hidden"
            class="form-control"
            dmx-bind:value="connectedUser.data.matricule" />

          <div class="form-group mb-3 row">
            <label for="select2" class="col-sm-2 col-form-label">Agent</label>
            <div class="col-sm-10">
              <select
                id="select2"
                class="form-select form-select-sm"
                dmx-bind:options="getEmployeByIdSection.data"
                optiontext="Nom"
                optionvalue="Matricule"
                name="IdEmploye"></select>
            </div>
          </div>
          <div class="d-flex justify-content-around">
            <button id="btn5" class="btn btn-sm btn-primary" type="submit">
              Enregistrer
            </button>
            <button
              id="btn6"
              class="btn btn-sm btn-warning"
              data-bs-dismiss="modal">
              Annuler
            </button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
