          "type": "text"
        },
        {
          "name": "matricule",
          "type": "number"
        },
        {
          "name": "id_sim",
          "type": "number"
        },
        {
          "name": "num_tel",
          "type": "number"
        },
        {
          "name": "affectedby",
          "type": "text"
        },
        {
          "name": "user_mat",
          "type": "number"
        }
      ],
      "type": "dbcustom_query"
    }
  }
