          "type": "number"
        },
        {
          "name": "sim_id",
          "type": "number"
        },
        {
          "name": "employe_id",
          "type": "number"
        },
        {
          "name": "tsp_id",
          "type": "number"
        },
        {
          "name": "deleted",
          "type": "text"
        },
        {
